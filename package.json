{
  "name": "full-stack_portfolio",
  "version": "1.0.0",
  "description": "Portfolio built to display skills",
  "main": "server/index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server/index.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakedykstra/Full-Stack_Portfolio.git"
  },
  "author": "Jacob Dykstra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jakedykstra/Full-Stack_Portfolio/issues"
  },
  "homepage": "https://github.com/jakedykstra/Full-Stack_Portfolio",
  "dependencies": {
    "concurrently": "^3.5.0",
    "cors": "^2.8.4",
    "create-react-app": "^1.5.2",
    "dotenv": "^6.2.0",
    "express": "^4.15.3",
    "express-graphql": "^0.7.1",
    "express-session": "^1.11.3",
    "graphql": "^14.0.2",
    "mongoose": "^4.11.1",
    "morgan": "*",
    "nodemailer": "^4.7.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "nodemon": "^1.11.0",
    "path-parser": "^2.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-radar": "^0.2.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0"
  }
}
